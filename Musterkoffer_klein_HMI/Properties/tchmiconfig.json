  "trigger": [
    {
      "event": "onInitialized",
      "actions": [
        {
          "objectType": "Condition",
          "active": true,
          "parts": [
            {
              "if": [
                {
                  "compare1": {
                    "objectType": "Symbol",
                    "valueType": "tchmi:general#/definitions/String",
                    "symbolExpression": "%f%TcHmi.Functions.Musterkoffer_klein_HMI.ResolutionChange()%/f%"
