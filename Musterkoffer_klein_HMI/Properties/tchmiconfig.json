                  "compare2": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:general#/definitions/String",
                    "value": "TRUE"
                  },
                  "compareOperator": "==",
                  "logic": null
                }
              ],
              "then": [
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "%ctrl%TcHmiTopLevelRegion::TargetContent%/ctrl%",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:framework#/definitions/ContentPath",
                    "value": "Pages/StartPageSmartphone.content"
                  },
                  "asyncWait": true
                },
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "%ctrl%NavigationRegion::TargetContent%/ctrl%",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:framework#/definitions/ContentPath",
                    "value": "Pages/Smartphone/NavigationSmartphone.content"
                  },
                  "asyncWait": true
                },
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "%ctrl%HeaderRegion::TargetContent%/ctrl%",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:framework#/definitions/ContentPath",
                    "value": "Pages/Smartphone/HeaderSmartphone.content"
                  },
                  "asyncWait": true
                },
                {
                  "objectType": "JavaScript",
                  "active": true,
                  "sourceLines": [
                    "console.log(window.innerWidth);",
                    "console.log(window.innerHeight);",
                    "console.log(window.devicePixelRatio);",
                    "",
                    "TcHmi.Symbol.write('ScreenHeight', TcHmi.SymbolType.Internal, window.innerHeight, ",
                    "                   function (data) {",
                    "                   \tif (data.error ===TcHmi.Errors.NONE){",
                    "                   \t}",
                    "                   \telse{",
                    "                   \t\tconsole.log('Error');",
                    "                   \t}",
                    "                   });",
                    "TcHmi.Symbol.write('ScreenWidth', TcHmi.SymbolType.Internal, window.innerWidth, ",
                    "                   function (data) {",
                    "                   \tif (data.error ===TcHmi.Errors.NONE){",
                    "                   \t}",
                    "                   \telse{",
                    "                   \t\tconsole.log('Error');",
                    "                   \t}",
                    "                   });",
                    "TcHmi.Symbol.write('PixelDensity', TcHmi.SymbolType.Internal, window.devicePixelRatio, ",
                    "                   function (data) {",
                    "                   \tif (data.error ===TcHmi.Errors.NONE){",
                    "                   \t}",
                    "                   \telse{",
                    "                   \t\tconsole.log('Error');",
                    "                   \t}",
                    "                   });"
                  ],
                  "asyncWait": true,
                  "waitMode": "Synchronous"
                }
              ]
            },
            {
              "else": [
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "%ctrl%TcHmiTopLevelRegion::TargetContent%/ctrl%",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:framework#/definitions/ContentPath",
                    "value": "Pages/StartPage.content"
                  },
                  "asyncWait": true
                },
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "%ctrl%NavigationRegion::TargetContent%/ctrl%",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:framework#/definitions/ContentPath",
                    "value": "Pages/Desktop/NavigationDesktop.content"
                  },
                  "asyncWait": true
                },
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "%ctrl%HeaderRegion::TargetContent%/ctrl%",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:framework#/definitions/ContentPath",
                    "value": "Pages/Desktop/HeaderDesktop.content"
                  },
                  "asyncWait": true
                },
                {
                  "objectType": "JavaScript",
                  "active": true,
                  "sourceLines": [
                    "console.log(window.innerWidth);",
                    "console.log(window.innerHeight);",
                    "console.log(window.devicePixelRatio);",
                    "",
                    "TcHmi.Symbol.write('ScreenHeight', TcHmi.SymbolType.Internal, window.innerHeight, ",
                    "                   function (data) {",
                    "                   \tif (data.error ===TcHmi.Errors.NONE){",
                    "                   \t}",
                    "                   \telse{",
                    "                   \t\tconsole.log('Error');",
                    "                   \t}",
                    "                   });",
                    "TcHmi.Symbol.write('ScreenWidth', TcHmi.SymbolType.Internal, window.innerWidth, ",
                    "                   function (data) {",
                    "                   \tif (data.error ===TcHmi.Errors.NONE){",
                    "                   \t}",
